class Person:
    # vars
    name = ""

    # init
    def __init__(self, name):
        self.name = name

    # defs
    def __str__(self):
        return self.name

class Dialogue:
    # vars
    name = ""

    # init
    def __init__(self, name):
        self.name = name

    # defs
    def __str__(self):
        return self.name
